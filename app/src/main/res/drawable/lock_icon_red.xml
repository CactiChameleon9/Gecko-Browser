<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:bottom="20dp" android:gravity="center">
        <shape android:shape="oval" >

            <size
                android:width="14dp"
                android:height="14dp" />

            <solid
                android:color="@color/colorPrimary"/>
            <stroke
                android:width="2dp"
                android:color="@color/colorPrimary" />
        </shape>
    </item>


    <item android:gravity="center">
        <shape android:shape="rectangle">

            <size
                android:width="20dp"
                android:height="19dp" />


            <corners android:radius="3dp"/>

            <solid
                android:color="@color/colorPrimary"/>
        </shape>

    </item>


    <item android:bottom="20dp" android:gravity="center">
        <shape android:shape="oval" >

            <size
                android:width="10dp"
                android:height="10dp" />

            <solid
                android:color="@color/colorPrimary"/>
            <stroke
                android:width="2dp"
                android:color="@color/colorPrimaryDark" />
        </shape>
    </item>

    <item android:gravity="center" android:bottom="10dp">
        <shape android:shape="rectangle" >

            <size
                android:height="10dp"
                android:width="10dp"/>

            <solid
                android:color="@color/colorPrimary"/>

            <stroke
                android:width="2dp"
                android:color="@color/colorPrimaryDark" />

        </shape>

    </item>

    <item android:gravity="center" android:bottom="10.5dp">
        <shape android:shape="rectangle" >

            <size
                android:height="10dp"
                android:width="6dp"/>

            <solid
                android:color="@color/colorPrimary"/>

        </shape>

    </item>



    <item android:gravity="center" android:bottom="20dp">
    <shape android:shape="oval" >

        <size
            android:height="6dp"
            android:width="6dp"/>

        <solid
            android:color="@color/colorPrimary"/>

    </shape>
    </item>


    <item android:gravity="center">
        <shape android:shape="rectangle">

            <size
                android:width="16dp"
                android:height="15dp" />


            <corners android:radius="1dp"/>

            <solid
                android:color="@color/colorPrimaryDark"/>
            <stroke
                android:width="2dp"
                android:color="@color/colorPrimaryDark" />
        </shape>

    </item>


    <item android:gravity="center" android:left="8dp" android:bottom="17.5dp">
        <shape android:shape="rectangle">

            <size
                android:width="3dp"
                android:height="2.5dp" />


            <solid
                android:color="@color/colorPrimary"/>
        </shape>

    </item>
</layer-list>